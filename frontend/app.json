{
  "expo": {
    "name": "Miluv",
    "slug": "miluv-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "miluv",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF0F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.miluv.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Miluv membutuhkan akses kamera untuk verifikasi wajah dan foto profil",
        "NSPhotoLibraryUsageDescription": "Miluv membutuhkan akses galeri untuk upload foto profil",
        "NSMicrophoneUsageDescription": "Miluv membutuhkan akses microphone untuk voice note",
        "NSLocationWhenInUseUsageDescription": "Miluv menggunakan lokasi Anda untuk menampilkan matches terdekat"
      }
    },
    "android": {
      "package": "com.miluv.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFF0F5"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFF0F5"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Miluv membutuhkan akses ke foto Anda untuk upload foto profil"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Miluv menggunakan lokasi Anda untuk menampilkan matches terdekat"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Miluv membutuhkan akses microphone untuk voice note"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "miluv-dating-app"
      }
    }
  }
}
